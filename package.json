{
  "name": "ad-webserver",
  "version": "0.1.0",
  "description": "simple ad web server for delivering creatives to users ",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "start": "npm run build && nodemon dist/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "serve": "npm run build && node dist/index.js",
    "test": "npm run build && mocha --require @babel/register",
    "test:integration": "cross-env NODE_ENV=test mocha --require @babel/register --recursive ./test/integration/",
    "test:unit": "cross-env NODE_ENV=test mocha --require @babel/register --recursive ./test/unit/",
    "lint": "./node_modules/.bin/eslint src/"
  },
  "author": "Luca GÃ¶ttle <lgoettle@stud.hs-heilbronn.de>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "@babel/node": "^7.14.9",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
